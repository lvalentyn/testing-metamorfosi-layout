- const socialList =['fb', 'tw', 'in', 'inst']

section.navbar.js-sticky
	.container
		.navbar__row
			a.navbar__logo(href="#home" aria-label='main logo')
			.navbar__burger.js-burger-btn
				span
			nav.navbar__navigation.js-burger-menu
				ul.navbar__list
					li.navbar__el
						a.navbar__link(href=`#home` aria-label=`home link`) home
					li.navbar__el
						a.navbar__link(href=`#partners` aria-label=`our partners link`) our partners
					li.navbar__el
						a.navbar__link(href=`#team` aria-label=`our team link`) our team
					li.navbar__el
						a.navbar__link(href=`#contact` aria-label=`contact us link`) contact us
				
				.navbar__footer
					.navbar__social-box
						each soc in socialList
							a.navbar__soc(href=`/` aria-label=`${soc} link` class=`navbar__soc--${soc}`)

					a.navbar__policy(href="/" aria-label='Privacy Policy') Privacy Policy
				